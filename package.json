{
  "name": "@tchain/tcabci-read-js-client",
  "version": "2.5.10",
  "description": "TransferChain Fastest Read Network WebSocket Client",
  "main": "client.js",
  "type": "module",
  "scripts": {
    "format": "prettier --write \"**/*.{js,md,json}\" --ignore-path .gitignore",
    "lint:fix": "eslint --fix",
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha  --require esm --timeout 5000",
    "test:all": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha  --require esm --timeout 5000 '*_test.js'",
    "test:coverage": "export NODE_ENV=test || SET \"NODE_ENV=test\" && nyc --reporter=text mocha  --require esm --timeout 5000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TransferChain/tcabci-read-js-client.git"
  },
  "files": [
    "*.d.ts",
    "constants.js",
    "client.js",
    "errors.js",
    "message.js",
    "transaction.js",
    "util.js",
    "websocket.js",
    "websocketOptions.js"
  ],
  "author": "TransferChain A.G <info@transferchain.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/TransferChain/tcabci-read-js-client/issues"
  },
  "homepage": "https://github.com/TransferChain/tcabci-read-js-client#readme",
  "packageManager": "pnpm@10.19.0",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/chai": "^5.2.3",
    "chai": "^6.2.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "esm": "^3.2.25",
    "express": "^5.2.1",
    "globals": "^16.5.0",
    "mocha": "^11.7.5",
    "nyc": "^17.1.0",
    "prettier": "^3.7.4",
    "unit.js": "^2.1.1",
    "ws": "^8.18.3"
  },
  "dependencies": {
    "reconnecting-websocket": "^4.4.0"
  }
}
